{
    "default_profiles": {
        "daemon": "optimade",
        "verdi": "optimade"
    },
    "CONFIG_VERSION": {
        "CURRENT": 1,
        "OLDEST_COMPATIBLE": 0
    },
    "profiles": {
        "optimade": {
            "AIIDADB_ENGINE": "postgresql_psycopg2",
            "AIIDADB_PASS": "{{ aiida_db_pass }}",
            "AIIDADB_NAME": "{{ aiida_db_name }}",
            "AIIDADB_HOST": "{{ aiida_db_host }}",
            "AIIDADB_BACKEND": "{{ aiida_db_backend }}",
            "AIIDADB_PORT": "{{ aiida_db_port }}",
            "default_user_email": "developers@aiida.net",
            "AIIDADB_REPOSITORY_URI": "file:///app/.aiida/repository-optimade",
            "AIIDADB_USER": "{{ aiida_db_user }}"
        }
    }
}
